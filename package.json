    "64": "./gen-assets/icon64.plasmo.png",
    "128": "./gen-assets/icon128.plasmo.png"
  },
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "./gen-assets/icon16.plasmo.png",
      "32": "./gen-assets/icon32.plasmo.png",
      "48": "./gen-assets/icon48.plasmo.png",
      "64": "./gen-assets/icon64.plasmo.png",
      "128": "./gen-assets/icon128.plasmo.png"
    },
    "default_popup": "./popup.html"
  },
  "version": "0.0.1",
  "author": "Luftjukie",
  "name": "DEV | Hacker wallet",
  "description": "It's a Ethereum Wallet made as a project.",
  "permissions": [
    "scripting",
    "unlimitedStorage",
    "nativeMessaging",
    "contextMenus",
    "activeTab",
    "tabs",
    "storage",
    "background",
    "sessions"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/src/contents/openPopupRelayer.ts"
      ]
    },
        {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/src/contents/index.ts"
      ]
    }
  ],
"background": {
    "service_worker": "background/index.ts"
  },
  "host_permissions": [
    "<all_urls>"
  ]
